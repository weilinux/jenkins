package com.mediahx.bean.stadium;

import java.util.List;

import com.mediahx.model.Stadium;

/**
 * 球场信息详情
 * @author PW
 *
 */
public class StadiumInfoBean {
	
	/**
	 * 球场信息
	 */
	private Stadium stadium;
	/**
	 * 时间
	 */
	private String time;
	
	/**
	 * 时间段详情list
	 */
	private List<StadiumDetailBean> stadiumDels;

	public Stadium getStadium() {
		return stadium;
	}

	public void setStadium(Stadium stadium) {
		this.stadium = stadium;
	}

	public String getTime() {
		return time;
	}

	public void setTime(String time) {
		this.time = time;
	}

	public List<StadiumDetailBean> getStadiumDels() {
		return stadiumDels;
	}

	public void setStadiumDels(List<StadiumDetailBean> stadiumDels) {
		this.stadiumDels = stadiumDels;
	}
	
}
